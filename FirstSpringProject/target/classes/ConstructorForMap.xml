<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">


	<bean id="myShop" class="com.DI.ConstructorInjectMap.Shop">
		<constructor-arg value="Ashish"></constructor-arg>
		<constructor-arg>
			<map>
				<entry key="Sugar" value="40"></entry>
				<entry key="Roce" value="60"></entry>
				<entry key="Oil" value="110"></entry>
				<entry key="Whey" value="60"></entry>
			</map>
		</constructor-arg>
	</bean>


</beans>